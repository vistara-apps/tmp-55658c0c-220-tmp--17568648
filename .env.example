# VibeFinder Environment Variables

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Authentication
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI
OPENROUTER_API_KEY=your_openrouter_api_key

# EnsembleData API
ENSEMBLEDATA_API_KEY=your_ensembledata_api_key

# SocialKit API
SOCIALKIT_API_KEY=your_socialkit_api_key

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Redis Cache
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Feature Flags
NEXT_PUBLIC_ENABLE_PREMIUM_FEATURES=false
NEXT_PUBLIC_ENABLE_SOCIAL_SHARING=true
NEXT_PUBLIC_ENABLE_LOCATION_TRACKING=true

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_vercel_analytics_id

